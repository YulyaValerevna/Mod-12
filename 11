money = int(input("Введите сумму"))

per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}

deposit = [int(money * per_cent.get('ТКБ') / 100), int(money * per_cent.get('СКБ') / 100),
           int(money * per_cent.get('ВТБ') / 100), int(money * per_cent.get('СБЕР') / 100)]

print(deposit)

i = sorted(deposit)
result = i [-1]

print("Максимальная сумма, которую вы можете заработать -", result)
